<%= form_with model: @user, local: true do |f| %>
<%= f.label :profile_name %>
<%= f.text_field :profile_name %>
<%= f.label :zipcode %>
<%= f.text_field :zipcode %>
<%= f.label :user_id %>
<%= f.number_field :user_id %>
<%= f.label :profile_picture %>
<%= f.file_field :profile_picture %>
<%= f.submit "Upload Photo" %>
<% end %>
